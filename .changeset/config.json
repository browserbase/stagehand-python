{
  "baseBranch": "main",
  "changelogFormat": "markdown",
  "access": "public",
  "commit": false,
  "changeTypes": {
    "major": {
      "description": "Breaking changes",
      "emoji": "💥"
    },
    "minor": {
      "description": "New features", 
      "emoji": "✨"
    },
    "patch": {
      "description": "Bug fixes",
      "emoji": "🐛"
    }
  },
  "package": {
    "name": "stagehand",
    "versionPath": "stagehand/__init__.py",
    "versionPattern": "__version__ = \"(.*)\"",
    "pyprojectPath": "pyproject.toml"
  }
}